/* 投放记录 */
.g-doc, page {
  height: 100%;
  position: relative;
}
 
.g-doc {
  height: 100%;
  position: relative;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  flex-flow: column;
}
 
.layer-top {
  height: 50px;
}
 
.layer-bottom {
  height: 50px;
}
 
.g-bd {
  width: 100%;
  background: transparent;
  -webkit-box-flex: 1;
  flex: 1;
  margin-bottom: 50px;
}
 
.g-ft {
  width: 100%;
  bottom: 0;
  position: fixed;
}
.table {
  display:flex;
  flex-direction:column;
  
  border-bottom:0;
}
.scrollClass {
  display: flex;
  width: 100%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: white;
}
.tr {
  display:flex;
}
.td {
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.table_header{
  height:90rpx;
}
.table_header .th{
  background:red;
  display:flex;
  flex-direction:column;
  flex: 1;
  height:90rpx;
  background:rgba(241, 252, 255, 1);
  border-right:1rpx solid rgba(218, 217, 217, 1);
  border-top:1rpx solid rgba(218, 217, 217, 1);
  border-bottom:1rpx solid rgba(218, 217, 217, 1);
  justify-content:center;
  align-items:center;
  overflow-x:auto;
  font-size:26rpx;
  color:rgba(74, 74, 74, 1);
  position:relative;
  z-index:999;
}
.td {
  display: flex;
  flex-direction: column;
  flex:1;
  background: white;
  justify-content: center;
  align-items: center;
  border: 1rpx solid rgba(218, 217, 217, 1);
  border-top: 0;
  border-left:0; 
}
.table_main .td{
 flex: 1;
  min-height: 100rpx;
  font-size: 30rpx;
  color: rgba(55, 134, 244, 1);
  
}
.btn-opt{
  text-align: center;
}
.btn-opt .weui-btn{
  margin-left: 50rpx;
}
.seeMore button{
  padding:0 1em;
  font-size:30rpx;
}
.materialList{
  max-height:500rpx;
  overflow:scroll;
}
.materialList .list{
  display: flex;
  width: 100%;
  padding-top:30rpx;
  border-bottom: 1rpx solid rgba(218, 217, 217, 1);
  align-items:flex-end;
}
.materialList .list .txt{
  width: 120rpx;
}
.materialList .list .val{
  flex: 1;
}
.noData{
  text-align: center;
  height:90rpx;
  font-size:26rpx;
  border-bottom: 1rpx solid rgba(218, 217, 217, 1);
  line-height:90rpx;
}
.sysGetSb{
  background: orange;
  color: #fff;
font-size: 40rpx;



}
button[size=mini] {
  padding: 10rpx 40rpx;
}

.tipsBox{
  display: flex;
  justify-content: space-between;
  margin: 30rpx;
}
.txtToltal{
  display: flex;
  font-size: 30rpx;
  color: #989898;
  align-items: center;
}